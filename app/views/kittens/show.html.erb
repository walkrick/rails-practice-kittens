<%= notice %>
<%= image_tag(@kitten.image) %>

<%= form_for @categorization, url: kitten_categorizations_path(@kitten) do |f| %>

  <%= f.collection_select(:id, @categories.reject { |category| @kitten.categories.include?(category) }, :id, :name, :prompt => "Select a Category") %>

  <%= f.submit "Create Categorization" %>


<% end %>


